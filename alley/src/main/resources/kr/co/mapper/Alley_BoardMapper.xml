<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.mapper.Alley_BoardMapper">

	<select id="getList" resultType="kr.co.domain.Alley_BoardVO">
		<![CDATA[
			select mainimage,storename,address 
			from alley_board where ano='22'
		]]>	
	</select>

	<select id="read" resultType="kr.co.domain.Alley_BoardVO">
		select * from alley_board where ano=#{ano}
	</select>


</mapper>

